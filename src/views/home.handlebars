<div class="container ">
  <div class="col-12 text-center ">
    
    <h1 id="h1-title"class="mb-5" >Lista de Productos</h1>
  </div>
  <div class="row row-cols-2 row-cols-md-2 g-4 text-center">
    {{#each products}}
      <div class="col">
        <div class="card">

          <div class="card-body">
            <h5 id="card-title""card-title  ">{{title}}</h5>
            <p id="card-description""card-text ">{{description}}</p>
            <p id="card-price">${{price}}</p>
          </div>

        </div>
      </div>
    {{/each}}
  </div>
</div>